{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "axe/aria": "off",
    "disown-opener": "off",
    "axe/text-alternatives": "off",
    "manifest-file-extension": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/structure": "off",
    "axe/forms": "off",
    "meta-charset-utf-8": "off",
    "compat-api/css": "off",
    "css-prefix-order": "off",
    "no-inline-styles": "off",
    "button-type": "off",
    "meta-viewport": "off"
  }
}